{
    "name": "fast-proj/professor-virtual",
    "description": "Professor Virtual - Chatbot.",
    "keywords": [],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "~8.0.0",
        "botman/botman": "2.6.1",
        "botman/driver-web": "1.5.1",
        "botman/tinker": "1.2.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.4",
        "laravel-notification-channels/webpush": "^5.1",
        "laravel/framework": "7.30.4",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^2.5.0",
        "pusher/pusher-php-server": "^4.1"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "beyondcode/laravel-dump-server": "^1.7.0",
        "filp/whoops": "~2.14.0",
        "fakerphp/faker": "^1.9.1",
        "lucascudo/laravel-pt-br-localization": "^1.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files":[
            "app/Helpers/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [

        ],
        "post-root-package-install": [

        ],
        "post-create-project-cmd": [

        ],
        "post-install-cmd": [
            "@php artisan clear-compiled",
            "chmod -R 777 public/"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
